function appendAllMatchingDivs() {
    try {
        // Get all div elements with the class 'tag_content_content_wrapper'
        var divChildren = document.querySelectorAll('div.tag_content_content_wrapper');

        // Get the uni-padding element
        var uniPadding = document.querySelector('.uni-padding');

        // Check if the uni-padding element exists
        if (uniPadding) {
            // Loop through each div with the 'tag_content_content_wrapper' class
            divChildren.forEach(function(divChild) {
                // Check if the div also has the class 'secondary_font'
                if (divChild.classList.contains('secondary_font')) {
                    // Append the divChild to the beginning of the uni-padding element
                    uniPadding.insertBefore(divChild, uniPadding.firstChild);
                }
            });
        }
    } catch (error) {
        console.error('An error occurred while appending matching divs:', error);
    }
}

// Call the function on page load
document.addEventListener('DOMContentLoaded', appendAllMatchingDivs);
</script>
